{
  "name": "@eid-easy/eideasy-signing-widget",
  "version": "0.1.0",
  "main": "dist/full/eideasy-signing-widget.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:regular": "vue-cli-service build",
    "build": "vue-cli-service build --mode production --dest dist/full --target lib --inline-vue --name eideasy-widget src/main.js",
    "build:external-vue": "vue-cli-service build --mode production --dest dist/widget-only --target lib --name eideasy-widget src/main.js",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@eid-easy/eideasy-js-sdk": "^0.13.0",
    "core-js": "^3.6.5",
    "lodash.defaultsdeep": "^4.6.1",
    "vue": "^2.6.11",
    "vue-custom-element": "^3.2.14",
    "vue-i18n": "^8.23.0",
    "vue-select": "^3.11.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-components": "warn",
      "vue/no-unused-vars": "warn",
      "vue/require-default-prop": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
